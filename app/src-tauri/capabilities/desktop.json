{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "desktop-capability",
  "windows": ["main"],
  "platforms": ["linux", "macOS", "windows"],
  "permissions": [
    "app:default",
    "event:default",
    "menu:default",
    "path:default",
    "process:default",
    "resources:default",
    "tray:default",
    "window:allow-set-title",
    "window:default",

    "dialog:allow-message",
    "dialog:allow-open",
    "process:allow-exit"
  ]
}
